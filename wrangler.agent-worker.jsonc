{
  "$schema": "node_modules/wrangler/config-schema.json",
  "main": "cloudflare/agents/agent-proxy-worker.ts",
  "name": "orchestrapi-agent-proxy-worker",
  "compatibility_date": "2025-07-07",
  "compatibility_flags": ["nodejs_compat", "global_fetch_strictly_public"],
  "ai": {
    "binding": "AI"
  },
  "vars": {
    "TMDB_API_KEY": "placeholder"
  },
  "durable_objects": {
    "bindings": [
      { "name": "OrchestrApiAgent", "class_name": "OrchestrApiAgent" }
    ]
  },
  "migrations": [
    {
      "tag": "v1",
      "new_sqlite_classes": ["OrchestrApiAgent"]
    }
  ],
  "observability": {
    "enabled": true,
    "head_sampling_rate": 1
  }
}
